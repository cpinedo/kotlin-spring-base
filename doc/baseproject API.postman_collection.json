	"item": [
		{
			"name": "auth",
			"item": [
				{
					"name": "POST auth/login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.environment.unset(\"jwtToken\");\r",
									"pm.environment.unset(\"refreshToken\");\r",
									"\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"jwt does exist\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.token).not(null);\r",
									"});\r",
									"\r",
									"pm.test(\"refresh token does exist\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.refreshToken).not(null);\r",
									"});\r",
									"\r",
									"pm.environment.set(\"jwtToken\", pm.response.json().token);\r",
									"pm.environment.set(\"refreshToken\", pm.response.json().refreshToken);\r",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"username\": \"user\",\r\n  \"password\": \"user\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseUrl}}/auth/login",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"auth",
								"login"
							]
						}
					},
					"response": []
				},
				{
					"name": "POST auth/refresh",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"jwt does exist\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.token).not(null);\r",
									"});\r",
									"\r",
									"pm.test(\"refresh token does exist\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.refreshToken).not(null);\r",
									"});\r",
									"\r",
									"if(pm.response.to.have.status(200)){\r",
									"    pm.environment.unset(\"jwtToken\");\r",
									"    pm.environment.unset(\"refreshToken\");\r",
									"\r",
									"    pm.environment.set(\"jwtToken\", pm.response.json().token);\r",
									"    pm.environment.set(\"refreshToken\", pm.response.json().refreshToken);\r",
									"}\r",
									"\r",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
								{
